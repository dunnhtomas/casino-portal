# Casino Portal Environment Configuration
# Copy this file to .env and fill in your actual API keys

# Google Cloud Custom Search API Configuration
# Get these from: https://console.cloud.google.com/
# Enable Custom Search API and create credentials
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_SEARCH_ENGINE_ID=your_search_engine_id_here

# AI APIs Configuration
# OpenAI API for content generation and image creation
OPENAI_API_KEY=sk-your_openai_api_key_here

# DeepSeek API for casino content generation
DEEPSEEK_API_KEY=sk-your_deepseek_api_key_here

# Application Configuration
NODE_ENV=development

# Optional: API Rate Limiting
GOOGLE_API_RATE_LIMIT=10
GOOGLE_API_MAX_RETRIES=3

# Optional: Casino Data Configuration
MAX_CASINOS_TO_PROCESS=100
ENABLE_LOGO_SCRAPING=true
ENABLE_REAL_TIME_UPDATES=false

# Optional: Security Configuration
ENABLE_CSRF_PROTECTION=true
SESSION_SECRET=your_session_secret_here

# Optional: Database Configuration (if needed)
# DATABASE_URL=your_database_url_here
# REDIS_URL=your_redis_url_here