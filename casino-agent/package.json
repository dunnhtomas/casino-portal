{
  "name": "casino-portal-agent",
  "version": "1.0.0",
  "description": "Autonomous executable agent for casino portal development",
  "main": "dist/UltimateSmartAgent.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/UltimateSmartAgent.js",
    "ultimate": "node dist/UltimateSmartAgent.js",
    "capabilities": "node dist/UltimateSmartAgent.js capabilities",
